    $(".about-us-abcd").removeAttr("onclick");

      function external_alert(callType) {
          if($("abcd").text()=="About Us"){
              alert("abcdef");}
        if (callType == 'toEnglish') {
          if (confirm("This will lead you to English language")) {
            // window.open("/MinistryV2/homepage.html",'_blank');
            window.open("/content/mca/global/en/home.html", '_self');
          };
        }
        else if (callType == 'toHindi') {
          if (confirm("This will lead you to हिंदी language")) {
            // window.open("/MinistryV2_hn/homepagehindi.html",'_blank');
            window.open("/content/mca/global/hi/home.html", '_self');
          };
        }
        else {
          return confirm("You are about to navigate to an external website");
        }
      }
